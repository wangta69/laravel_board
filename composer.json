{
    "name": "wangta69/laravel_board",
    "description": "BBS bulletin board system",
	"keywords": ["laravel", "bbs", "pondol" , "board"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pondol",
            "email": "wangta69@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
		"kyslik/column-sortable": "^6.0",
        "wangta69/php-thumbnail": "^1"
    },
    "autoload": {
        "psr-4": {
            "Pondol\\Bbs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pondol\\Bbs\\BbsServiceProvider"
            ],
            "aliases": {
                "CustomPackage": "Pondol\\Bbs\\Facades\\BbsFacade"
            }
        }
    }
}
